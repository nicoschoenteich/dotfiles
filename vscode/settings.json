{
	"telemetry.enableTelemetry": false,
	"workbench.colorTheme": "Deepdark Material Theme | Default Version",
	"workbench.iconTheme": "vscode-icons-mac",
	"workbench.startupEditor": "none",
	"sap.ux.applicationModeler.disableMigrationPrompt": true,
	"editor.semanticTokenColorCustomizations": {
		"[Default Dark+]": {
			"rules": {
				"property.mandatory:xml": {
					"bold": true
				},
				"string.i18n:xml": {
					"italic": true
				}
			}
		},
		"[Default Light+]": {
			"rules": {
				"property.mandatory:xml": {
					"bold": true
				},
				"string.i18n:xml": {
					"italic": true
				}
			}
		},
		"[Default High Contrast]": {
			"rules": {
				"property.mandatory:xml": {
					"bold": true
				},
				"string.i18n:xml": {
					"italic": true
				}
			}
		}
	},
	"cSpell.userWords": [
		"approuter",
		"Middlewares",
		"webcomponents"
	],
	"[xml]": {
		"editor.defaultFormatter": "DotJoshJohnson.xml"
	},
	"editor.minimap.enabled": false,
	"breadcrumbs.enabled": false,
	"workbench.statusBar.visible": false,
	"sap.ux.applicationModeler.showPropertiesDescriptionInPageEditor": true,
	"sap.ux.help.showDescriptions": "enabled",
	"sap.ux.help.guideDetailsMode": "full",
	"editor.largeFileOptimizations": false,
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": [
				"f",
				"e"
			],
			"commands": [
				"workbench.files.action.focusFilesExplorer"
			]
		},
		{
			"before": [
				"f",
				"b"
			],
			"commands": [
				":bnext"
			]
		},
		{
			"before": [
				"f",
				"w"
			],
			"commands": [
				"workbench.action.focusNextGroup"
			]
		},
		{
			"before": [
				"f",
				"t"
			],
			"commands": [
				"workbench.action.terminal.focus"
			]
		},
		{
			"before": [
				"l",
				"f"
			],
			"commands": [
				"editor.action.formatDocument"
			]
		}
	]
}
